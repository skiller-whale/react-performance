    "test:coverage": "npm test -- --coverage",
    "test:ts:coverage": "npm run test:ts -- --coverage",
    "serve:build": "serve -s build -l 4000",
    "lint": "eslint . --ext ts,tsx,js,jsx --fix",
    "compile": "tsc --jsx preserve && npm run lint"
